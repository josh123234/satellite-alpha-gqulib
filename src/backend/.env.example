NODE_ENV=development
PORT=3000
API_PREFIX=api/v1
CORS_ORIGIN=*
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
LOG_LEVEL=debug
REQUEST_TIMEOUT=30000

# Authentication and Security
JWT_SECRET=your-secret-key
JWT_EXPIRES_IN=1d
JWT_REFRESH_SECRET=your-refresh-secret
JWT_REFRESH_EXPIRES_IN=7d
PASSWORD_SALT_ROUNDS=10
API_KEY_HEADER=X-API-Key

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_S3_BUCKET=your-bucket-name
AWS_CLOUDWATCH_GROUP=saas-management
AWS_SQS_QUEUE_URL=your-sqs-url
AWS_SNS_TOPIC_ARN=your-sns-topic

# PostgreSQL Master Configuration
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=password
DB_DATABASE=saas_management
DB_SCHEMA=public
DB_SSL=false
DB_LOGGING=false
DB_POOL_SIZE=10
DB_IDLE_TIMEOUT=10000
DB_CONNECTION_TIMEOUT=2000
DB_STATEMENT_TIMEOUT=60000

# PostgreSQL Replication Configuration
DB_REPLICATION_MODE=true
DB_MASTER_HOST=master-host
DB_MASTER_PORT=5432
DB_SLAVE_HOSTS=slave1,slave2
DB_SLAVE_PORTS=5432,5432
DB_READ_REPLICA_WEIGHTS=1,1

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=saas_management
MONGODB_USER=mongodb-user
MONGODB_PASSWORD=mongodb-password
MONGODB_AUTH_SOURCE=admin
MONGODB_REPLICA_SET=rs0
MONGODB_MIN_POOL_SIZE=5
MONGODB_MAX_POOL_SIZE=10
MONGODB_CONNECTION_TIMEOUT=20000

# Redis Cache Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=password
REDIS_DB=0
REDIS_TTL=3600
REDIS_KEY_PREFIX=smp:
REDIS_CLUSTER_MODE=false
REDIS_CLUSTER_NODES=host1:6379,host2:6379
REDIS_MAX_RETRIES=3
REDIS_RETRY_INTERVAL=1000
REDIS_POOL_SIZE=100

# AI Service Configuration
TENSORFLOW_MODEL_PATH=/models/tensorflow
PYTORCH_MODEL_PATH=/models/pytorch
OPENAI_API_KEY=your-openai-key
AI_MODEL_VERSION=1.0.0
AI_REQUEST_TIMEOUT=30000
AI_MAX_TOKENS=2048
AI_TEMPERATURE=0.7
AI_MODEL_TYPE=gpt-4

# Service Discovery
SERVICE_REGISTRY_URL=http://localhost:8500
SERVICE_NAME=saas-management-api
SERVICE_VERSION=1.0.0
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=5